definition
	PERSON[at0000.1.1] matches {    -- Patient
		details matches {
			allow_archetype ITEM_TREE[at0001.0.1] matches {    -- Patient Data
				include
					archetype_id/value matches {/openEHR-DEMOGRAPHIC-ITEM_TREE\.person_details_ruphasoft\.v0/}
			}
		}
		identities cardinality matches {1..*; unordered} matches {
			allow_archetype PARTY_IDENTITY[at0002.1.1] matches {    -- Name
				include
					archetype_id/value matches {/openEHR-DEMOGRAPHIC-PARTY_IDENTITY\.person_name_ruphasoft\.v0/}
			}
		}
		contacts cardinality matches {1..1; unordered} matches {
			CONTACT[at0003.1.1] matches {    -- Contacts
				addresses cardinality matches {1..*; unordered} matches {
					ADDRESS[at0042.0.1] occurrences matches {0..1} matches {    -- Primary Contacts
						details matches {
							ITEM_TREE[at0043] matches {    -- Item tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0044.0.1] occurrences matches {0..1} matches {    -- Mobile Phone
										value matches {
											DV_IDENTIFIER matches {*}
										}
									}
									ELEMENT[at0046.0.1] occurrences matches {0..1} matches {    -- Email Address
										value matches {
											DV_IDENTIFIER matches {*}
										}
									}
									ELEMENT[at0.0.1] occurrences matches {0..1} matches {    -- Primary Contact
										value matches {
											DV_BOOLEAN matches {*}
										}
									}
								}
							}
						}
					}
					allow_archetype ADDRESS[at0.0.9] occurrences matches {0..1} matches {    -- ADDRESS
						include
							archetype_id/value matches {/openEHR-DEMOGRAPHIC-ADDRESS\.address\.v0/}
					}
				}
			}
		}
		relationships cardinality matches {1..*; unordered} matches {
			PARTY_RELATIONSHIP[at0004.1] matches {    -- Relationships
				details matches {
					ITEM_TREE[at0005] matches {    -- ITEM_TREE
						items cardinality matches {0..*; unordered} matches {
							ELEMENT[at0.0.10] occurrences matches {0..1} matches {    -- Type
								value matches {
									DV_CODED_TEXT matches {*}
									DV_TEXT matches {*}
								}
							}
						}
					}
					ITEM_TREE[at0.40] matches {*}    -- Personal relationship
				}
			}
			PARTY_RELATIONSHIP[at0.2] matches {    -- Third party payer
				details matches {
					ITEM_TREE[at0.20] matches {    -- Patient identifiers
						items cardinality matches {0..*; unordered} matches {
							allow_archetype CLUSTER[at0.21.1] matches {    -- Healthcare consumer identifier.
								include
									archetype_id/value matches {/openEHR-DEMOGRAPHIC-CLUSTER\.person_identifier_ruphasoft\.v0/}
							}
						}
					}
				}
			}
			PARTY_RELATIONSHIP[at0.3] matches {    -- Healthcare provider/Health professional
				details matches {
					ITEM_TREE[at0.30] matches {    -- Patient identifiers
						items cardinality matches {0..*; unordered} matches {
							allow_archetype CLUSTER[at0.31] matches {    -- Patient identifier
								include
									archetype_id/value matches {/openEHR-DEMOGRAPHIC-CLUSTER\.person_identifier[a-zA-Z0-9_-]*\.v0/}
							}
						}
					}
				}
			}
		}
	}

